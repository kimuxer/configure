!-*-{Xft settings
Xft.dpi:  	96
Xft.antialias:	True
Xft.hinting:	Full
Xft.rgba:       rgb
!-*-}
!-*-{字体设置
URxvt.font:             xft:monaco:size=10,xft:apple:size=10
URxvt.boldFont:         xft:monaco:bold:size=10,xft:apple:bold:size=10
URxvt.italicFont:       xft:monaco:size=10,xft:apple:size=10
URxvt.boldItalicFont:   xft:monaco:bold:size=10,xft:apple:bold:size=10
!-*-}

!-*-{默认设置
URxvt.termName:                 rxvt-unicode-256color
URxvt.depth:                    32
URxvt.buffered:                 True
URxvt.jumpScroll:               True 
URxvt.geometry:                 80x25+100+100

URxvt.scrollBar:                False
URxvt.scrollTtyOutput:          False
Urxvt.scrollTtyKeypress:        True
URxvt.scrollWithBuffer:         False
URxvt.scrollstyle:              plain
Urxvt.secondaryScroll:          True
URxvt.internalBorder:           1
URxvt.saveLines:                8192
URxvt.borderLess:               True
URxvt.cursorBlink:              False
URxvt.perl-ext-common:          default,matcher,url-select,keyboard-select
URxvt.matcher.button:           1
URxtt.transparent:              True
URxvt.shading:                  80
URxvt.preeditType:              OverTheSpot
URxvt.inputMethod:              fcitx   
URxvt.urlLauncher:              chromium
URxvt.borderColor:              #CCCCCC
URxvt.thickness:                0
URxvt.inheritPixmap:            False

!-*-}
URxvt.keysym.M-u:               perl:url-select:select_next
URxvt.keysym.M-Escape:          perl:keyboard-select:activate
tRxvt.keysym.M-s:               perl:keyboard-select:search

